{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ApiKey": "Env:ApiKeyCL",
  "ConnectionStrings": {
    "DbContext": "Env:ConnectionStrings__DbContext"
  },
  "Jwt": {
    "Secret": "Env:Jwt__Secret",
    "Issuer": "Env:Jwt__Issuer",
    "Audience": "Env:Jwt__Audience",
    "AccessTokenExpireMinutes": 15,
    "RefreshTokenExpireDays": 7
  },
  "BaseUrl": "Env:BaseUrl",
  "ClAuctionApi": {
    "BaseUrl": "Env:ClAuctionApi__BaseUrl",
    "ApiKey": "Env:ClAuctionApi__ApiKey",
    "BearerToken": "Env:ClAuctionApi__BearerToken"
  },
  "ClaveUnica": {
    "ClientId": "Env:ClaveUnica__ClientId",
    "ClientSecret": "Env:ClaveUnica__ClientSecret",
    "RedirectUri": "Env:ClaveUnica__RedirectUri",
    "AuthorizationEndpoint": "Env:ClaveUnica__AuthorizationEndpoint",
    "TokenEndpoint": "Env:ClaveUnica__TokenEndpoint",
    "UserInfoEndpoint": "Env:ClaveUnica__UserInfoEndpoint",
    "AllowAutoUserCreation": "Env:ClaveUnica__AllowAutoUserCreation"
  },
  "AllowedHosts": "*"
}
